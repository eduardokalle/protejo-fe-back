{
    "version": 2,
    "name": "protejo-fe-backv2",
    "builds": [{
        "src": "index.js",
        "use": "@now/node-server"
    }],
    "routes": [
        {"src": "/*", "methods": ["GET"], "dest": "/index.js"},
        {"src": "/sendMail", "methods": ["POST"], "dest": "/routes.js"}
    ],
    "env": {
        "NODE_ENV": "production",
        "PORT": "80"
    }
}